<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
	   <hibernate-mapping>
	   		<class name="com.yeoou.tour.model.Shopper" table="t_shopper" lazy="false">
	   			<id name="shopperId" column="ShopId">
	   				<generator class="uuid.hex"></generator>
	   			</id>
	   			<property name="name" column="Name"></property>
	   			<property name="shopName" column="ShopName"></property>
	   			<property name="mobile" column="Mobile"></property>
	   			<property name="phone" column="Phone"></property>
	   			<property name="fax" column="Fax"></property>
	   			<property name="address" column="Address"></property>
	   			<property name="qq" column="QQ"></property>
	   			<property name="msn" column="Msn"></property>
				<property name="email" column="Email"></property>
	   			<property name="logoUrl" column="LogoUrl"></property>
	   			<property name="isTest" column="Test"></property>
	   			<property name="areaId" column="AreaId"></property>
	   			<property name="userId" column="UserId"></property>
	   			<property name="parentId" column="ParentId"></property>
	   			<property name="pub" column="Pub"></property>
	   		</class>
	   </hibernate-mapping>
