<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
	<hibernate-mapping>
		<class name="com.yeoou.tour.model.KeyWord" table="t_keypoint" lazy="false">
			<id name="kpId" column="KpId">
				<generator class="uuid.hex"></generator>
			</id>
			<property name="name" column="Name"></property>
			<property name="url" column="Url"></property>
			<property name="id" column="Id"></property>
			<property name="same" column="Same"></property>
			<property name="areaId" column="areaId"></property>
		</class>
	</hibernate-mapping>
