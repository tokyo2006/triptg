        <from>^/visa/visaDetail/(\w+).html$</from>
        <to type="forward">/visa/visaDetail.shtml?visaId=$1</to>
    </rule>
   
<!--// 签证系统  --> 
<!-- 首页跳转 -->
    <rule>
        <from>^/index.html$</from>
        <to type="forward">/homePage/index.shtml</to>
    </rule>
	<rule>
